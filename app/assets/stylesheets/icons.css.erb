@font-face {
	font-family: 'csw-icons';
	src:url(<%= asset_path 'csw-icons.eot' %>);
	src:url(<%= asset_path 'csw-icons.eot?#iefix' %>) format('embedded-opentype'),
		url(<%= asset_path 'csw-icons.woff' %>) format('woff'),
		url(<%= asset_path 'csw-icons.ttf' %>) format('truetype'),
		url(<%= asset_path 'csw-icons.svg#icons' %>) format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="csw-icon-"], [class*="csw-icon-"] {
	font-family: 'csw-icons';
	position: relative;
	top: 1px;
	display: inline-block;
	text-align: center;
	font-size: 1.1em;
	font-style: normal;
	font-weight: 400;
	line-height: 1;
	width: 0.9em;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

